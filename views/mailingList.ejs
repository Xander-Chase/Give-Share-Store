<div class="container mt-4">
    <h1>Mailing List</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% people.forEach(function(person) { %>
                <tr>
                    <td><%= person.name %></td>
                    <td><%= person.email %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>