<head>
    <style>
        body {
            padding-bottom: 3vh;
        }
    </style>
</head>
<div class="container mt-4">
    <span class="admin-header">Mailing List</span>
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% people.forEach(function(person) { %>
                <tr>
                    <td><%= person.firstName + " " + person.lastName %></td>
                    <td><%= person.email %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>
